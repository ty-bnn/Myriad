import installPackage from "./sample/sample02.ty"

copyFileFromLocal(filePath) {
    if (filePath == "./hello.go") {
        WORKDIR /app
        COPY . ./hello.go
    } else if (filePath == "./good.go") {
        WORKDIR /app
        COPY . ./good.go
    } else {
        WORKDIR /app
        COPY . ./else.go
    }
}

main(arg) {
    FROM python:${arg}
    installPackage("httpd")
    copyFileFromLocal("./hello.go")
    CMD ["python", "-m", "http.server", "--cgi"]
}
