dockerfile() {
    {{-
        FROM ubuntu:latest
        WORKDIR /user
        COPY . .
        RUN go build .
        CMD ["./server"]
    -}}
}

main() {
    {{-
        FROM ubuntu:aaa
        RUN ...
    -}}
    data := JsonUnmarshal("./test/versions.json")
    for (version in data.keys) {
        versionData := data[version]
        for (variant in  versionData["variants"]) {
            output := "./test/" + version + "/" + variant + ".dockerfile"
            output << {
                dockerfile()
            }
        }
    }
    {{-
        COPY . .
    -}}
}